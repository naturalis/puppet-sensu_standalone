{
  "checks": {
    "<%= @check_name -%>": {
      "command": "<%= @command -%>",
      "handlers": [
      <% @handler.each do |handler| -%>
        "<%=handler %>"
      <% end -%>
      ],
      "interval": <%= @interval -%>,
      "occurrences": <%= @occurrences -%>,
      "refresh": <%= @refresh -%>,
      "standalone": true,
      "subscribers": [
      <% @subscribers.each do |subscription| -%>
        "<%=subscription %>"
      <% end -%>
      ]
    }
  }
}


